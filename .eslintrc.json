{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
  "ecmaVersion": 2020,
  "sourceType": "module",
  "project": [
  "tsconfig.json"
  ]
  },
  "env": {
  "es2017": true,
  "browser": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      { "endOfLine": "auto" }
    ]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ]
 }